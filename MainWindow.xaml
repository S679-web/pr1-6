<Window x:Class="pr12_UP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pr12_UP"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Content="Галерея изображений" Margin="0,0,0,10" Padding="10" Click="OpenImageGallery_Click"/>
            <Button Content="Дорожные знаки" Padding="10" Click="OpenRoadSigns_Click"/>
        </StackPanel>
    </Grid>
</Window>
